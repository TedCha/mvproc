{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "MultiValue PROC",
	"patterns": [
		{ "include": "#labels" },
		{ "include": "#numbers" },
		{ "include": "#strings" },
		{ "include": "#proc-bang" },
		{ "include": "#comments" },
		{ "include": "#commands" }
	],
	"repository": {
		"commands": {
			"patterns": [
				{ "include": "#a-cmd" },
				{ "include": "#call-cmd"},
				{ "include": "#d-cmd" },
				{ "include": "#go-cmd" },
				{ "include": "#h-cmd" },
				{ "include": "#if-cmd" },
				{ "include": "#ih-cmd" },
				{ "include": "#ip-cmd" },
				{ "include": "#is-cmd" },
				{ "include": "#link-cmd" },
				{ "include": "#o-cmd" },
				{ "include": "#p-cmd" },
				{ "include": "#ri-cmd" },
				{ "include": "#s-cmd" },
				{ "include": "#t-cmd" },
				{ "include": "#x-cmd"},
				{ "include": "#nullary-commands" }
		]
		},
		"nullary-commands": {
			"comment": "Nullary Commands",
			"match": "(?:^[^HOX]*\\b)(B|BO|F|IT|RO|SP|SS|STOFF|ST OFF|STON|ST ON)\\b",
			"captures": {
				"1": { "name": "entity.name.function.mvproc" }
			}
		},
		"comments": {
			"comment": "Line Comment",
			"match": "\\b(C.*)$",
			"captures": {
				"1": { "name": "comment.line.mvproc" }
			}
		},
		"labels": {
			"comment": "Numeric Label",
			"match": "^(\\d+)\\b",
			"captures": {
				"1": { "name": "keyword.control.mvproc"}
			}
		},
		"numbers": {
			"comment": "Numeric Constants",
			"match": "(\\d+)",
			"captures": {
				"1": { "name": "constant.numeric.mvproc" }
			}
		},
		"strings": {
			"comment": "Quoted Strings",
			"name": "string.double.mvproc",
			"match": "(\"[^\"\n]+\")",
			"captures": {
				"1": { "name": "string.double.mvproc" }
			}
		},
		"proc-bang": {
			"comment": "mvBase PROC Bang",
			"match": "^(PQ)$",
			"captures": {
				"1": { "name": "constant.language.mvproc" }
			}
		},
		"a-cmd": {
			"comment": "A Command",
			"match": "\\b(A)(\"|')?(\\d+)?(?:,(\\d+))?\\b",
			"captures": {
				"1": { "name": "entity.name.function.mvproc" },
				"2": { "name": "constant.character.mvproc" },
				"3": { "name": "variable.parameter.mvproc" },
				"4": { "name": "variable.parameter.mvproc" }
			}
		},
		"call-cmd": {
			"comment": "Call Command []",
			"match": "\\B[\\[](?:(DICT) )?([^ ]+| )(?: ([^ ]+))?[\\]](?: (\\d+))?\\B",
			"captures": {
				"1": { "name": "storage.type" },
				"2": { "name": "string.unquoted.mvproc" },
				"3": { "name": "variable.parameter.mvproc" },
				"4": { "name": "keyword.control.mvproc" }
			}
		},
		"d-cmd": {
			"comment": "D Command",
			"match": "\\b(D)(\\d+)?(?:,(\\d+))?([+])?\\B",
			"captures": {
				"1": { "name": "entity.name.function.mvproc" },
				"2": { "name": "variable.parameter.mvproc" },
				"3": { "name": "variable.other.mvproc" },
				"4": { "name": "constant.character.mvproc" }
			}
		},
		"go-cmd": {
			"comment": "GO/G Command",
			"match": "(?<!\\w)(GOTO|GO|G)(?: (\\d+)| (A)(\\d+)?)?",
			"captures": {
				"1": { "name": "entity.name.function.mvproc" },
				"2": { "name": "keyword.control.mvproc" },
				"3": { "name": "entity.name.function.mvproc" },
				"4": { "name": "variable.parameter.mvproc" }
			}
		},
		"h-cmd": {
			"comment": "H Command",
			"match": "(?<!\\w)(H)(.*?)(?:(?=[<]+$)|(?=$))(<$)?",
			"captures": {
				"1": { "name": "entity.name.function.mvproc" },
				"2": { "name": "string.unquoted.mvproc"},
				"3": { "name": "constant.character.mvproc" }
			}
		},
		"if-cmd": {
			"comment": "IF Command",
			"match": "(?<!\\w)(IF)(?: (#)?(?: )?)?(?:(A)(\\d+)?(?:[^\\d]?(?<=,)(\\d+))?|(E)|(S))?(?:[ ]?(<|>|=|#|\\[|\\])(?:[ ]?(\\(?[^ ]+\\)?)))?",
			"captures": {
				"1": { "name": "entity.name.function.mvproc" },
				"2": { "name": "constant.character.mvproc" },
				"3": { "name": "entity.name.function.mvproc" },
				"4": { "name": "variable.parameter.mvproc" },
				"5": { "name": "variable.parameter.mvproc" },
				"6": { "name": "entity.name.function.mvproc" },
				"7": { "name": "entity.name.function.mvproc" },
				"8": { "name": "constant.character.mvproc"},
				"9": { "name": "string.unquoted.mvproc" }
			}
		},
		"ih-cmd": {
			"comment": "IH Command",
			"match": "(?<!\\w)(IH)(.*$)",
			"captures": {
				"1": { "name": "entity.name.function.mvproc" },
				"2": { "name": "string.unquoted.mvproc"}
			}
		},
		"ip-cmd": {
			"comment": "IP Command",
			"match": "(?<!\\w)(IP)(B)?(.)?$",
			"captures": {
				"1": { "name": "entity.name.function.mvproc" },
				"2": { "name": "variable.parameter.mvproc" },
				"3": { "name": "constant.character.mvproc"}
			}
		},
		"is-cmd": {
			"comment": "IS Command",
			"match": "(?<!\\w)(IS)(.)?$",
			"captures": {
				"1": { "name": "entity.name.function.mvproc" },
				"2": { "name": "keyword.other.mvproc"}
			}
		},
		"link-cmd": {
			"comment": "Link Command ()",
			"match": "(?<!\\w)[(](?:(DICT) )?([^ ,]+)(?: ([^ ,]+))?[)](?: (\\d+))?",
			"captures": {
				"1": { "name": "storage.type" },
				"2": { "name": "string.unquoted.mvproc" },
				"3": { "name": "variable.parameter.mvproc" },
				"4": { "name": "keyword.control.mvproc" }
			}
		},
		"o-cmd": {
			"comment": "O Command",
			"match": "(?<!\\w)(O)(.*?)(?:(?=[+]+$)|(?=$))([+]$)?",
			"captures": {
				"1": { "name": "entity.name.function.mvproc" },
				"2": { "name": "string.unquoted.mvproc"},
				"3": { "name": "constant.language.mvproc" }
			}
		},
		"p-cmd": {
			"comment": "P Command",
			"match": "(?<!\\w)(P)(?!Q)(H|P|W|X)?",
			"captures": {
				"1": { "name": "entity.name.function.mvproc" },
				"2": { "name": "variable.parameter.mvproc" }
			}
		},
		"ri-cmd": {
			"comment": "RI Command",
			"match": "(?<!\\w)(RI)(\\d+)?",
			"captures": {
				"1":{ "name": "entity.name.function.mvproc" },
				"2": { "name": "variable.parameter.mvproc" }
			}
		},
		"s-cmd": {
			"comment": "S Command",
			"match": "(?<!\\w)(S)(\\d+)",
			"captures": {
				"1":{ "name": "entity.name.function.mvproc" },
				"2": { "name": "variable.parameter.mvproc" }
			}
		},
		"t-cmd": {
			"comment": "T Command",
			"match": "(?<!\\w)(T)(?:,(B|C))?",
			"captures": {
				"1": { "name": "entity.name.function.mvproc" },
				"2": { "name": "variable.parameter.mvproc" }
			}
		},
		"x-cmd": {
			"comment": "X Command",
			"match": "(?<!\\w)(X)(.+)?",
			"captures": {
				"1": { "name": "entity.name.function.mvproc" },
				"2": { "name": "string.unquoted.mvproc"}
			}
		}
	},
	"scopeName": "source.mvproc"
}