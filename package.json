{
	"name": "mvproc",
	"displayName": "MultiValue PROC",
	"description": "",
	"version": "0.0.1",
	"publisher": "tcharts-boop",
	"engines": {
		"vscode": "^1.54.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/tcharts-boop/mvproc"
	},
	"categories": [
		"Programming Languages",
		"Language Packs",
		"Linters"
	],
	"activationEvents": [
		"onLanguage:mvproc"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "mvproc",
				"aliases": [
					"MultiValue PROC",
					"mvproc"
				],
				"extensions": [
					".proc"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "mvproc",
				"scopeName": "source.mvproc",
				"path": "./syntaxes/mvproc.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.54.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"ts-loader": "^8.0.14",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	},
	"dependencies": {
		"lodash": "^4.17.21"
	}
}
